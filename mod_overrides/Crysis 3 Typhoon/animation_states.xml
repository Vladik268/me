<?xml version="1.0"?>
<tweaks>
	<tweak version="2" name="anims/fps/fps_spawn" extension="animation_states">
		<search>
			<xml/>
			<state name="fps/reload"/>
		</search>
		<target mode="append">
			<state name="fps/reload/typhoon" type="once" segment="base" speed="1.0">
				<anim name="typhoon_reload"/>
				<keys>
					<key at="enter" callback="anim_clbk_stop_weapon_magazine_empty" class_name="base" param1="reload"/>	
					<key at="trigger" trigger="typhoon_reload_start" callback="play_sound" class_name="base" param1="typhoon_reload_start"/>
					<key at="trigger" trigger="typhoon_slider_release" callback="play_sound" class_name="base" param1="typhoon_slider_release"/>
					<key at="trigger" trigger="typhoon_latch_release" callback="play_sound" class_name="base" param1="typhoon_latch_release"/>	
					<key at="trigger" trigger="typhoon_handle_hit_1" callback="play_sound" class_name="base" param1="typhoon_handle_hit_1"/>
					<key at="trigger" trigger="typhoon_handle_hit_2" callback="play_sound" class_name="base" param1="typhoon_handle_hit_2"/>	
					<key at="trigger" trigger="typhoon_lift_open" callback="play_sound" class_name="base" param1="typhoon_lift_open"/>	
					<key at="trigger" trigger="typhoon_beep1" callback="play_sound" class_name="base" param1="typhoon_signal_lift_up"/>
					<key at="trigger" trigger="typhoon_beep2" callback="play_sound" class_name="base" param1="typhoon_signal_lift_up"/>
					<key at="trigger" trigger="typhoon_foley1" callback="play_sound" class_name="base" param1="typhoon_foley_2"/>
					<key at="trigger" trigger="typhoon_beep3" callback="play_sound" class_name="base" param1="typhoon_signal_lift_up"/>
					<key at="trigger" trigger="typhoon_beep4" callback="play_sound" class_name="base" param1="typhoon_signal_lift_up"/>
					<key at="trigger" trigger="typhoon_maggrab1" callback="play_sound" class_name="base" param1="typhoon_mag_grab_1"/>
					<key at="trigger" trigger="typhoon_mag_lock_release" callback="play_sound" class_name="base" param1="typhoon_mag_lock_release"/>
					<key at="trigger" trigger="typhoon_beep5" callback="play_sound" class_name="base" param1="typhoon_signal_lift_up"/>
					<key at="trigger" trigger="typhoon_foley2" callback="play_sound" class_name="base" param1="typhoon_foley_4"/>
					<key at="trigger" trigger="typhoon_maggrab2" callback="play_sound" class_name="base" param1="typhoon_mag_grab_1"/>
					<key at="trigger" trigger="typhoon_mag_slide_out_1" callback="play_sound" class_name="base" param1="typhoon_mag_slide_out_1"/>
					<key at="trigger" trigger="typhoon_maggrab3" callback="play_sound" class_name="base" param1="typhoon_mag_grab_2"/>
					<key at="trigger" trigger="typhoon_mag_slide_out_2" callback="play_sound" class_name="base" param1="typhoon_mag_slide_out_2"/>
					<key at="trigger" trigger="typhoon_mag_slide_out_3" callback="play_sound" class_name="base" param1="typhoon_mag_slide_out_3"/>
					<key at="trigger" trigger="typhoon_foley_turn" callback="play_sound" class_name="base" param1="typhoon_foley_1"/>	
					<key at="trigger" trigger="typhoon_mag_contact_2" callback="play_sound" class_name="base" param1="typhoon_mag_contact_2"/>
					<key at="trigger" trigger="typhoon_mag_contact_1" callback="play_sound" class_name="base" param1="typhoon_mag_contact"/>	
					<key at="trigger" trigger="typhoon_mag_lock_engage" callback="play_sound" class_name="base" param1="typhoon_mag_lock_engage"/>	
					<key at="trigger" trigger="typhoon_mag_in" callback="play_sound" class_name="base" param1="typhoon_mag_in"/>
					<key at="trigger" trigger="typhoon_mag_in_2" callback="play_sound" class_name="base" param1="typhoon_mag_in_2"/>
					<key at="trigger" trigger="typhoon_foley3" callback="play_sound" class_name="base" param1="typhoon_foley_1"/>
					<key at="trigger" trigger="typhoon_lift_close_foley" callback="play_sound" class_name="base" param1="typhoon_lift_close_foley"/>
					<key at="trigger" trigger="typhoon_lift_close" callback="play_sound" class_name="base" param1="typhoon_lift_close"/>
					<key at="trigger" trigger="typhoon_foley4" callback="play_sound" class_name="base" param1="typhoon_foley_3"/>
					<key at="trigger" trigger="typhoon_boop" callback="play_sound" class_name="base" param1="typhoon_signal_lift_down"/>
					<key at="trigger" trigger="typhoon_pull1" callback="play_sound" class_name="base" param1="typhoon_slider_pull"/>
					<key at="trigger" trigger="typhoon_top_close" callback="play_sound" class_name="base" param1="typhoon_top_close"/>
					<key at="trigger" trigger="typhoon_foley5" callback="play_sound" class_name="base" param1="typhoon_foley_2"/>
					<key at="trigger" trigger="typhoon_pull2" callback="play_sound" class_name="base" param1="typhoon_slider_pull"/>
					<key at="trigger" trigger="typhoon_reload_finish" callback="play_sound" class_name="base" param1="typhoon_reload_finish"/>
				</keys>
				<exit name="fps/idle"/>
			</state>
		</target>
	</tweak>

